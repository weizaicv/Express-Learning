<%- include('../includes/head.ejs')%>
<%- include('../includes/navigation.ejs')%>
<main>
    <p>订单</p>
    <% if (orders.length<=0) { %>
        <h1>no orders</h1>
    <% } else { %>    
        <ul>
            <% orders.forEach(order=>{%>
                <li>
                    #<% order.id %>
                    <ul>
                        <% order.products.forEach(product => {%>
                        <li><%= product.title%>-<%= product.orderItem.quanlity %></li>
                        <% }) %>
                    </ul>
                </li>
            <% }) %>
        </ul>
    <% } %>    
</main>
<%- include('../includes/end.ejs')%>

